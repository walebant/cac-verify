{
  "name": "cac-verify",
  "version": "1.1.2",
  "description": "Search and Verify if a company is registered in Nigeria",
  "main": "build/index.js",
  "scripts": {
    "format": "prettier --write src/",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "npx tsc",
    "start": "node build/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q build/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walebant/cac-verify.git"
  },
  "author": "Ashiru Olawale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/walebant/cac-verify/issues"
  },
  "homepage": "https://github.com/walebant/cac-verify#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.3"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "javascript"
  ],
  "files": [
    "build/**/*"
  ]
}
